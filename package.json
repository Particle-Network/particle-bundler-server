{
    "name": "particle-bundler-server",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "build": "nest build",
        "tsc": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "ENVIRONMENT=dev nest start --watch",
        "start:debug": "ENVIRONMENT=debug nest start --debug --watch",
        "start:prod": "ENVIRONMENT=production node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "ENVIRONMENT=dev jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@account-abstraction/sdk": "^0.6.0",
        "@ethereumjs/tx": "^5.0.0",
        "@liaoliaots/nestjs-redis": "^9.0.5",
        "@nestjs/axios": "^3.0.0",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/mongoose": "^9.2.1",
        "@nestjs/platform-fastify": "^10.1.2",
        "@nestjs/schedule": "^2.1.0",
        "async-await-mutex-lock": "^1.0.10",
        "axios": "^1.4.0",
        "bn.js": "^5.2.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "ethers": "^6.7.1",
        "fastify": "4.20.0",
        "fastify-raw-body": "^4.2.1",
        "ioredis": "^5.2.4",
        "lodash": "^4.17.21",
        "mongoose": "^6.7.2",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "ts-enum-util": "^4.0.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/axios": "^0.14.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "@types/lodash": "^4.14.196",
        "@types/node": "^20.3.1",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "test",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
